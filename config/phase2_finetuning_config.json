{
  "batch_size": 32,
  "learning_rate": 5e-04,
  "num_epochs": 10,
  "temperature": 0.2,
  "negative_sampling_ratio": 0.7,
  "pathway_weight": 0.8,
  "use_pathway_negatives": true,
  "use_view_augmentation": false,
  "checkpoint_frequency": 500,
  "log_frequency": 10,
  "shuffle_data": true,
  "text_encoder_model": "sentence-transformers/all-mpnet-base-v2",
  "text_encoder_device": null,
  "embedding_cache_size": 10000,
  "enable_embedding_preload": false,
  "clear_cache_between_epochs": true,
  "max_resume_views": 5,
  "max_job_views": 5,
  "fallback_on_augmentation_failure": true,
  "hard_negative_max_distance": 2.0,
  "medium_negative_max_distance": 4.0,
  "esco_graph_path": "training_output/career_graph_bridged_complete.gexf",
  "esco_kg_path": "dataset/esco/esco_kg.gexf",
  "global_negative_sampling": true,
  "global_negative_pool_size": 1000,
  "freeze_text_encoder": true,
  "projection_dim": 128,
  "projection_dropout": 0.3,
  "weight_decay": 0.001,
  "training_phase": "fine_tuning",
  "use_augmentation_labels_only": false,
  "augmentation_positive_ratio": 1.0,
  "pretrained_model_path": "phase1_pretraining/best_checkpoint.pt",
  "freeze_contrastive_layers": true,
  "classification_dropout": 0.3,
  "max_negatives_per_anchor": 20,
  "validation_path": "preprocess/data_splits_v3/validation.jsonl",
  "validate_every_n_epochs": 1,
  "ontology_weight": 0.3,
  "ot_distance_scale": 10.0,
  "pos_class_weight": 2.5
}
